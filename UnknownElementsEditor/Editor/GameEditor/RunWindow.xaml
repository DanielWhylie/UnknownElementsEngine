<Window x:Class="UnknownElementsEditor.Editor.GameEditor.RunWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UnknownElementsEditor.Editor.GameEditor"
        xmlns:project="clr-namespace:UnknownElementsEditor.GameProject"
        mc:Ignorable="d" WindowStartupLocation="CenterScreen"
        d:DataContext="{d:DesignInstance Type=project:UserProject, IsDesignTimeCreatable=True}"
        Title="RunWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto"/>
        </Grid.ColumnDefinitions>

        <StackPanel x:Name="viewPortContainer">
            <Image Width="1510" Height="700" x:Name="viewPort" Loaded="StartGraphics" HorizontalAlignment="Left"/>
        </StackPanel>
    </Grid>
</Window>
